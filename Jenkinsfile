#!groovy

node {
    
	
	milestone 1
	stage "Prepare environment"
        checkout scm
	    
	milestone 2
	    stage "Checkout and build deps"
	    sleep 30
	
	milestone 3
	    stage "Test and validate"
	    sleep 30
	
	milestone 4
	    stage "Test and validate"
	    sleep 30


    stage "Cleanup"
        deleteDir()
}
